<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  <PropertyGroup>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <SchemaVersion>2.0</SchemaVersion>
    <ProjectGuid>a1b2c3d4-e5f6-7890-abcd-ef1234567890</ProjectGuid>
    <ProjectHome>.</ProjectHome>
    <StartupFile>start_rasa.py</StartupFile>
    <SearchPath>
    </SearchPath>
    <WorkingDirectory>.</WorkingDirectory>
    <OutputPath>.</OutputPath>
    <Name>Rasa4</Name>
    <RootNamespace>Rasa4</RootNamespace>
    <InterpreterId>MSBuild|venv|$(MSBuildProjectFullPath)</InterpreterId>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DebugSymbols>true</DebugSymbols>
    <EnableUnmanagedDebugging>false</EnableUnmanagedDebugging>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DebugSymbols>true</DebugSymbols>
    <EnableUnmanagedDebugging>false</EnableUnmanagedDebugging>
  </PropertyGroup>
  <ItemGroup>
    <!-- Fichiers Python des actions -->
    <Compile Include="actions\__init__.py" />
    <Compile Include="actions\actions.py" />
    
    <!-- Fichiers Python des composants personnalisés -->
    <Compile Include="components\__init__.py" />
    <Compile Include="components\*.py" />
    
    <!-- Fichier de démarrage principal -->
    <Compile Include="start_rasa.py" />
  </ItemGroup>
  <ItemGroup>
    <!-- Fichiers de configuration YAML -->
    <Content Include="config.yml" />
    <Content Include="credential.yml" />
    <Content Include="endpoints.yml" />
    <Content Include="requirements.txt" />
    
    <!-- Fichiers de domaine -->
    <Content Include="domain\*.yml" />
    
    <!-- Fichiers de données d'entraînement -->
    <Content Include="data\*.yml" />
    <Content Include="data\*.md" />
    
    <!-- Scripts PowerShell -->
    <Content Include="setup-rasa-env-fixed.ps1" />
    <Content Include="start-rasa-actions.ps1" />
    <Content Include="start-rasa-full.ps1" />
    <Content Include="start-rasa-server.ps1" />
    
    <!-- Fichiers environnement et configuration -->
    <Content Include=".env" />
    <Content Include=".gitignore" />
    <Content Include="test.txt" />
    <Content Include="Je souhaite soumettre une demande d.txt" />
  </ItemGroup>
  <ItemGroup>
    <!-- Dossiers du projet -->
    <Folder Include="actions\" />
    <Folder Include="components\" />
    <Folder Include="data\" />
    <Folder Include="domain\" />
    <Folder Include="models\" />
    <Folder Include="results\" />
    <Folder Include="interface\" />
    <Folder Include=".rasa\" />
  </ItemGroup>
  <ItemGroup>
    <!-- Environnement virtuel -->
    <Interpreter Include="venv\">
      <Id>venv</Id>
      <Version>3.9</Version>
      <Description>Environnement virtuel Rasa (Python 3.9)</Description>
      <InterpreterPath>Scripts\python.exe</InterpreterPath>
      <WindowsInterpreterPath>Scripts\pythonw.exe</WindowsInterpreterPath>
      <PathEnvironmentVariable>PYTHONPATH</PathEnvironmentVariable>
      <Architecture>X64</Architecture>
    </Interpreter>
  </ItemGroup>
  <Import Project="$(MSBuildExtensionsPath32)\Microsoft\VisualStudio\v$(VisualStudioVersion)\Python Tools\Microsoft.PythonTools.targets" />
  <Target Name="BeforeBuild">
  </Target>
  <Target Name="AfterBuild">
  </Target>
</Project>